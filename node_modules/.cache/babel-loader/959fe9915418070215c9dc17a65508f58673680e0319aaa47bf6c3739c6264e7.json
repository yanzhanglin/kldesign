\n        <span class=\"bar\"></span>\r\n        <span class=\"bar\"></span>\r\n        <span class=\"bar\"></span>\r\n      </div>\r\n    </nav>\r\n  </template>\r\n  \r\n  <script setup>\r\n  import { ref } from 'vue'\r\n  \r\n  // 响应式菜单状态\r\n  const isMenuOpen = ref(false)\r\n  \r\n  // 导航菜单数据\r\n  const navItems = [\r\n    { title: '首页', path: '/' },\r\n    { title: '案例', path: '/cases' },\r\n    { title: '团队', path: '/team' },\r\n    { title: '服务', path: '/services' },\r\n    { title: '联系', path: '/contact' }\r\n  ]\r\n  \r\n  // 切换移动端菜单\r\n  const toggleMenu = () => {\r\n    isMenuOpen.value = !isMenuOpen.value\r\n  }\r\n  </script>\r\n  \r\n  <style lang=\"scss\" scoped>\r\n  // 迪士尼主题变量\r\n  $disney-blue: #0d47a1;\r\n  $gold: #ffd700;\r\n  $magic-shadow: 0 4px 15px rgba(0,0,0,0.2);\r\n  \r\n  .disney-nav {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding: 1rem 2rem;\r\n    background: linear-gradient(135deg, darken($disney-blue, 10%) 0%, $disney-blue 100%);\r\n    box-shadow: $magic-shadow;\r\n    position: relative;\r\n  \r\n    // Logo样式\r\n    .nav-logo {\r\n      img {\r\n        height: 50px;\r\n        transition: transform 0.3s ease;\r\n  \r\n        &:hover {\r\n          transform: scale(1.1) rotate(-5deg);\r\n        }\r\n      }\r\n    }\r\n  \r\n    // 导航链接容器\r\n    .nav-links {\r\n      display: flex;\r\n      gap: 2rem;\r\n      list-style: none;\r\n      margin: 0;\r\n      padding: 0;\r\n  \r\n      // 移动端样式\r\n      @media (max-width: 768px) {\r\n        position: absolute;\r\n        top: 100%;\r\n        left: 0;\r\n        right: 0;\r\n        flex-direction: column;\r\n        background: rgba($disney-blue, 0.95);\r\n        max-height: 0;\r\n        overflow: hidden;\r\n        transition: max-height 0.3s ease-out;\r\n  \r\n        &.active {\r\n          max-height: 500px; // 根据实际内容调整\r\n        }\r\n      }\r\n    }\r\n  \r\n    // 单个导航链接\r\n    .nav-link {\r\n      color: white;\r\n      text-decoration: none;\r\n      font-size: 1.1rem;\r\n      position: relative;\r\n      padding: 0.5rem 1rem;\r\n      transition: all 0.3s ease;\r\n  \r\n      // 激活状态样式\r\n      &.router-link-active {\r\n        color: $gold;\r\n        &::after {\r\n          content: '';\r\n          position: absolute;\r\n          bottom: 0;\r\n          left: 50%;\r\n          transform: translateX(-50%);\r\n          width: 80%;\r\n          height: 2px;\r\n          background: $gold;\r\n        }\r\n      }\r\n  \r\n      // 悬浮特效\r\n      &:hover {\r\n        color: $gold;\r\n        transform: translateY(-2px);\r\n  \r\n        .star {\r\n          opacity: 1;\r\n          transform: scale(1);\r\n        }\r\n      }\r\n  \r\n      // 小星星动画\r\n      .star {\r\n        position: absolute;\r\n        top: -10px;\r\n        right: -10px;\r\n        width: 8px;\r\n        height: 8px;\r\n        background: white;\r\n        clip-path: polygon(50% 0%, 61% 35%, 98% 35%, 68% 57%, 79% 91%, 50% 70%, 21% 91%, 32% 57%, 2% 35%, 39% 35%);\r\n        opacity: 0;\r\n        transform: scale(0);\r\n        transition: all 0.3s ease;\r\n      }\r\n    }\r\n  \r\n    // 汉堡菜单样式\r\n    .hamburger {\r\n      display: none;\r\n      cursor: pointer;\r\n      padding: 0.5rem;\r\n  \r\n      @media (max-width: 768px) {\r\n        display: block;\r\n      }\r\n  \r\n      .bar {\r\n        display: block;\r\n        width: 25px;\r\n        height: 3px;\r\n        margin: 5px auto;\r\n        background: white;\r\n        transition: all 0.3s ease;\r\n      }\r\n  \r\n      // 展开状态动画\r\n      &.active {\r\n        .bar:nth-child(1) {\r\n          transform: translateY(8px) rotate(45deg);\r\n        }\r\n        .bar:nth-child(2) {\r\n          opacity: 0;\r\n        }\r\n        .bar:nth-child(3) {\r\n          transform: translateY(-8px) rotate(-45deg);\r\n        }\r\n      }\r\n    }\r\n  }\r\n  </style>"],"mappings":";OAKaA,UAAuB;;EAH3BC,KAAK,EAAC;AAAY;;;uBAF3BC,mBAAA,CAAAC,SAAA,SACI