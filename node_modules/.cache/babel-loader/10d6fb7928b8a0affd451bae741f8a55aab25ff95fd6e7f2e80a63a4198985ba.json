 } from 'vue-router'\r\n  import CaseCard from '@/components/CaseCard.vue'\r\n  \r\n  // 路由实例\r\n  const router = useRouter()\r\n  \r\n  // 模拟案例数据\r\n  const caseList = ref([\r\n    {\r\n      id: 1,\r\n      title: '梦幻城堡设计',\r\n      description: '融合巴洛克与现代主义的魔法城堡概念设计，使用Blender完成全流程制作',\r\n      tags: ['3D建模', '概念设计', '灯光艺术'],\r\n      image: '/images/cases/castle.jpg'\r\n    },\r\n    {\r\n      id: 2,\r\n      title: '魔法森林场景',\r\n      description: '沉浸式自然场景搭建，包含动态天气系统和粒子特效',\r\n      tags: ['场景设计', 'UE5', '特效'],\r\n      image: '/images/cases/forest.jpg'\r\n    },\r\n    // 添加更多案例...\r\n  ])\r\n  \r\n  // 查看详情处理\r\n  const handleViewDetails = (caseId) => {\r\n    router.push(`/cases/${caseId}`) // 假设已配置动态路由\r\n  }\r\n  </script>\r\n  \r\n  <style lang=\"scss\" scoped>\r\n  .cases-page {\r\n    padding: 2rem;\r\n    min-height: 100vh;\r\n    background: linear-gradient(45deg, #2b1055, #7597de);\r\n  \r\n    .page-title {\r\n      color: white;\r\n      text-align: center;\r\n      font-size: 2.5rem;\r\n      margin-bottom: 3rem;\r\n      text-shadow: 0 0 15px rgba(255, 215, 0, 0.5);\r\n    }\r\n  \r\n    .case-grid {\r\n      display: grid;\r\n      grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));\r\n      gap: 2rem;\r\n      max-width: 1200px;\r\n      margin: 0 auto;\r\n    }\r\n  }\r\n  \r\n  /* 移动端适配 */\r\n  @media (max-width: 768px) {\r\n    .cases-page {\r\n      padding: 1rem;\r\n      \r\n      .page-title {\r\n        font-size: 2rem;\r\n        margin-bottom: 2rem;\r\n      }\r\n      \r\n      .case-grid {\r\n        grid-template-columns: 1fr;\r\n      }\r\n    }\r\n  }\r\n  </style>"],"mappings":";;EAESA,KAAK,EAAC;AAAY;;EAKhBA,KAAK,EAAC;AAAW;;uBALxBC,mBAAA,CAaM,OAbNC,UAaM,GAZJC,mBAAA,UAAa,E,0BACbC,mBAAA,CAAqC;IAAjCJ,KAAK,EAAC;EAAY,GAAC,WAAS,sBAEhCG,mBAAA,YAAe,EACfC,mBAAA,CAOM,OAPNC,UAOM,I,kBANJJ,mBAAA,CAKEK,SAAA,QAbVC,WAAA,CAS6BC,MAAA,CAAAC,QAAQ,EAApBC,QAAQ;yBADjBC,YAAA,CAKEH,MAAA;MAHCI,GAAG,EAAEF,QAAQ,CAACG,EAAE;MAChBC,QAAQ,EAAEJ,QAAQ;MACl